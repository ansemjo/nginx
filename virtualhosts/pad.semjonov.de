server {

    listen 443;
    listen [::]:443;

    server_name pad.semjonov.de;

    root    /var/www/common;
    index   index.html index.htm;

    include params.d/proxy;
    include snippets/ssl_letsencrypt;
    include locations/enabled/common/*;
    include locations/enabled/pad.semjonov.de/*;

}
