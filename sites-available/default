server {
        listen 80 default_server;
        server_name nas.semjonov.de;
	rewrite_log on;
#	rewrite ^ https://$server_name$request_uri? permanent;
#	rewrite (http:\/\/)nas.semjonov.de\/+(.+) https://$server_name$request_uri? permanent;
	rewrite ^[\/]+[git|emby|sysusage|owncloud]+[\/]*(.*)$ https://$server_name$request_uri? last;
#	rewrite ^(.*)$ https://$server_name$request_uri? permanent;
	
	root	/var/www;
	index	index.php index.html index.htm;

	include		locations/includeall;
}

